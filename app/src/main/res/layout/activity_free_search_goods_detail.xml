        android:gravity="center_vertical"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="80px">

            <TextView
                android:layout_marginLeft="30px"
                android:textSize="26px"
                android:textColor="@color/a99"
                android:text="Payment："
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/tv_freesearchgoodsdetail_payway"
                android:textSize="26px"
                android:textColor="@color/a33"
                android:text="【支付宝】"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />


    </LinearLayout>
    <TextView
        android:background="@color/e6"
        android:layout_width="match_parent"
        android:layout_height="1px" />
    <LinearLayout
        android:gravity="center_vertical"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="80px"
        android:visibility="gone">

            <TextView
                android:layout_marginLeft="30px"
                android:textSize="26px"
                android:textColor="@color/a99"
                android:text="Others："
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/tv_freesearchgoodsdetail_other"
                android:textSize="26px"
                android:textColor="@color/a33"
                android:text="99%"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />



    </LinearLayout>
    <TextView
        android:background="@color/e6"
        android:layout_width="match_parent"
        android:layout_height="1px" />
    <LinearLayout
        android:orientation="vertical"
        android:gravity="center_vertical"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="80px">

        <TextView
            android:id="@+id/tv_freesearchgoodsdetail_chakandetail"
            android:layout_gravity="right"
            android:gravity="center"
            android:background="@drawable/shengqingshouhou_null"
            android:layout_marginRight="23px"
            android:textSize="28px"
            android:textColor="@color/a4d4d4d"
            android:text="Details"
            android:layout_width="160px"
            android:layout_height="60px" />




    </LinearLayout>
