            android:layout_weight="1"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="35px"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/tv_freesearchgoodsdetail_bianhao"
                android:text="找货编号：123654789"
                android:textSize="24px"
                android:textColor="@color/a99"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/tv_freesearchgoodsdetail_date"
                android:layout_marginTop="6px"
                android:text="生成时间：12:30:56			2017-03-02"
                android:textSize="24px"
                android:textColor="@color/a99"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </LinearLayout>
        <TextView
            android:id="@+id/tv_freesearchgoodsdetail_lainxikefu"
            android:layout_marginRight="23px"
            android:layout_gravity="center"
